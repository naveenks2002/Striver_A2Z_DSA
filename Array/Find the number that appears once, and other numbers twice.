MY METHOD 1: 
 int singleNumber(vector<int>& nums) {
        int ans;
        int max_count = 0;
        for(int i=0; i<nums.size(); i++){
             if(count(nums.begin(),nums.end(),nums[i])==1)
                return nums[i];
        }
        return -1;
    }
   
MY METHOD 2:
int singleNumber(vector<int>& arr) {

        unordered_map <int,int> hash;

        for(auto x: arr)
        	hash[x]++;
        
        
        for(auto x: arr){
        	if(hash[x]==1)
        			return x;
        }
        return 0;       
    }
